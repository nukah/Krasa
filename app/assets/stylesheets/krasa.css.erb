@charset "utf-8";
/* CSS Document */
body {
	font-family: Georgia;
	color: #67949B;
	font-size: 12px;
	min-height: 100%;
}
a {
	color: #007B89;
	text-decoration: underline;
}
a.cupid-green {
	background-color: #7fbf4d;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#7fbf4d), to(#63a62f));
	/* Saf4+, Chrome */
	background-image: -webkit-linear-gradient(top, #7fbf4d, #63a62f);
	background-image: -moz-linear-gradient(top, #7fbf4d, #63a62f);
	background-image: -ms-linear-gradient(top, #7fbf4d, #63a62f);
	background-image: -o-linear-gradient(top, #7fbf4d, #63a62f);
	background-image: linear-gradient(top, #7fbf4d, #63a62f);
	border: 1px solid #63a62f;
	border-bottom: 1px solid #5b992b;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
	-webkit-box-shadow: inset 0 1px 0 0 #96ca6d;
	-moz-box-shadow: inset 0 1px 0 0 #96ca6d;
	-ms-box-shadow: inset 0 1px 0 0 #96ca6d;
	-o-box-shadow: inset 0 1px 0 0 #96ca6d;
	box-shadow: inset 0 1px 0 0 #96ca6d;
	color: #fff;
	font: bold 11px "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Geneva, Verdana, sans-serif;
	line-height: 1;
	text-decoration: none;
	padding: 7px 10px 8px 10px;
	text-align: center;
	text-shadow: 0 -1px 0 #4c9021;
	width: 150px;
	height: 20px;
}
a.cupid-green:hover {
	background-color: #76b347;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#76b347), to(#5e9e2e));
	/* Saf4+, Chrome */
	background-image: -webkit-linear-gradient(top, #76b347, #5e9e2e);
	background-image: -moz-linear-gradient(top, #76b347, #5e9e2e);
	background-image: -ms-linear-gradient(top, #76b347, #5e9e2e);
	background-image: -o-linear-gradient(top, #76b347, #5e9e2e);
	background-image: linear-gradient(top, #76b347, #5e9e2e);
	-webkit-box-shadow: inset 0 1px 0 0 #8dbf67;
	-moz-box-shadow: inset 0 1px 0 0 #8dbf67;
	-ms-box-shadow: inset 0 1px 0 0 #8dbf67;
	-o-box-shadow: inset 0 1px 0 0 #8dbf67;
	box-shadow: inset 0 1px 0 0 #8dbf67;
	cursor: pointer;
}
a.cupid-green:active {
	border: 1px solid #5b992b;
	border-bottom: 1px solid #538c27;
	-webkit-box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eeeeee;
	-moz-box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eeeeee;
	-ms-box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eeeeee;
	-o-box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eeeeee;
	box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eeeeee;
}

.clean-gray {
	background-color: #eeeeee;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#eeeeee), to(#cccccc));
	/* Saf4+, Chrome */
	background-image: -webkit-linear-gradient(top, #eeeeee, #cccccc);
	background-image: -moz-linear-gradient(top, #eeeeee, #cccccc);
	background-image: -ms-linear-gradient(top, #eeeeee, #cccccc);
	background-image: -o-linear-gradient(top, #eeeeee, #cccccc);
	background-image: linear-gradient(top, #eeeeee, #cccccc);
	border: 1px solid #ccc;
	border-bottom: 1px solid #bbb;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
	color: #333;
	font: bold 11px "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Geneva, Verdana, sans-serif;
	line-height: 1;
	text-decoration: none;
	padding: 5px 15px 5px 15px;
	text-align: center;
	text-shadow: 0 1px 0 #eee;
}
.clean-gray:hover {
	background-color: #dddddd;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#dddddd), to(#bbbbbb));
	/* Saf4+, Chrome */
	background-image: -webkit-linear-gradient(top, #dddddd, #bbbbbb);
	background-image: -moz-linear-gradient(top, #dddddd, #bbbbbb);
	background-image: -ms-linear-gradient(top, #dddddd, #bbbbbb);
	background-image: -o-linear-gradient(top, #dddddd, #bbbbbb);
	background-image: linear-gradient(top, #dddddd, #bbbbbb);
	border: 1px solid #bbb;
	border-bottom: 1px solid #999;
	cursor: pointer;
	text-shadow: 0 1px 0 #ddd;
}
.clean-gray:active {
	border: 1px solid #aaa;
	border-bottom: 1px solid #888;
	-webkit-box-shadow: inset 0 0 5px 2px #aaaaaa, 0 1px 0 0 #eeeeee;
	-moz-box-shadow: inset 0 0 5px 2px #aaaaaa, 0 1px 0 0 #eeeeee;
	-ms-box-shadow: inset 0 0 5px 2px #aaaaaa, 0 1px 0 0 #eeeeee;
	-o-box-shadow: inset 0 0 5px 2px #aaaaaa, 0 1px 0 0 #eeeeee;
	box-shadow: inset 0 0 5px 2px #aaaaaa, 0 1px 0 0 #eeeeee;
}


/* Overall Layout */
#search {
	color: #648a14;
	padding-left: 5px;
	display: inline;
	font-size: 14px;
}

.wrap {
	background: url(<%= asset_path 'layout/fon.png' %>) no-repeat top;
}
.footer-wrap {
	height: 120px;
	vertical-align: bottom;
	background: url(<%= asset_path('layout/bg_footer.jpg') %>) no-repeat scroll center bottom;
}
#copyright {

}
#copyright .title {
	font-size: 12px;
}
#copyright .krasa {
	color: #648A14;
}
#copyright .online {
	color: #007B89;
}
#copyright .phones {
	font-size: 11px;
}
#developer {

}
#developer_title {
	font-size: 11px;
}
#developer_logo {
	position: absolute;
	right: 0;
	top: 0;
}

#search_form_keywords {
	width: 400px;
	height: 24px;
	background: url(<%= asset_path('layout/search.png') %>) no-repeat;
	border: none;
	padding: 0 0 0 5px;
}
#search_form_type_options {
	color: gray;
	padding-bottom: 0px;
}
#search_form_submit {
	padding: 1px 0 0 0;
}
#search_example {
	font-size: 12px;
	color: #648a14;
	padding-left: 50px;
}
#search_example a {
	color: orange;
	text-decoration: none;
}

#center-block {
	vertical-align: top;
	margin: 5px;
}

.sidebar {
	width: 20%;
}


.navigation dt {
	background: url(<%= asset_path('layout/icon_duxi.png') %>) no-repeat scroll 0 0 transparent;
	line-height: 23px;
}
dl.navigation dt a {
	color: #648A14;
	font-size: 1.2em;
	line-height: 1.5;
	font-weight: bold;
	padding-left: 20px;
}
dl.navigation dd a {
	padding-left: 40px;
	font-size: 13px;
	line-height: 1.2;
}
dl.navigation a.current {
	font-weight: bold;
	font-size: 115%;
	line-height: 20px;
	color: #7133C7;
}

/* Product Page */
h1 {
	font-size: 24px;
	font-variant: small caps;
	color: olive;
	text-shadow: 1px 1px 0px #eeeeee;
	word-spacing: 5px;
	letter-spacing: 1px;
}

#breadcrumbs {
	font-family: Georgia;
	fong-weight: bold;
	font-size: 12px;
}
#breadcrumbs a {
	color: olive;
	font-size: 13px;
	text-decoration: none;
}
#breadcrumbs a:hover {
	text-decoration: underline;
}
#breadcrumbs #product_link a {
	font-size: 115%;
	font-weight: bold;
}
#search_results_not_found {
	font-size: 16px;
	color: olive;
	text-align: center;
	text-shadow: 0px 1px 1px #4d4d4d;
}
.product-listing {
	list-style-type: none;
	align: left;
}
.product_properties {
	display: block;
	size: 200px;
}
span.option {
	color: black;
	font-weight: bold;
}
span.value {
	color: olive;
}
.product_description {
	font-size: 13px;
	text-indent: 1em;
	white-space: normal;
	display: inline;
}
.product_description p:first-letter {
	font-size: 14px;
	color: gray;
	text-decoration: underline;
}
.product_cart {
	margin-top: 10px;
}
.product_item {
	width: 130px;
	height: 130px;
	margin-right: 15px;
	margin-left: 15px;
	float: left;
	background-color: white;
}
.product_item .product-small-image {
	border: 1px solid #A6D1D6;
	height: 125px;
	padding: 1px;
	text-align: center;
	width: 125px;
}
.product_item .product-title {
    color: #007B89;
    font-size: 12px;
    line-height: 14px;
    height: 45px;
    overflow: hidden;
    padding-top: 5px;
}
.product_item .product-price {
    position: relative;
}
.product_item .product-price .price {
    color: #BA38A2;
    font-size: 14px;
    font-weight: bold;
}
.product {
	padding-top: 10px;
	position: relative;
	clear: both;
	width: 100%;
	display: inline-block;
	font-size: 14px;
	margin: 0;
}
.images {
	display: block;
}
.images > img {
	width: 100%;
	-webkit-box-shadow: 0px 3px 3px 2px rgba(0,0,0,0.3);
	-moz-box-shadow: 0px 3px 3px 2px rgba(0,0,0,0.3);
	box-shadow: 0px 3px 3px 2px rgba(0,0,0,0.3);
	border: 1px solid black;
}
.common_information {
	display: block;
	padding-left: 20px;
}
.product_thumbnails {
	margin-top: 15px;
}

.main_image {
	display: block;
	float: left;
	width: 100%;
	padding-right: 10px;
	margin: 5px;
}
.stock  {
	line-height: 20px;
}
.stock span {
	vertical-align: middle;
}
.stock img {
	width: 20px;
	height: 20px;
	vertical-align: top;
}
.variants_wrapper {
	display: inline;
}
#product_variants {
	margin-top: 5px;
	padding: 5px 10px 5px 0px;
	display: inline;
	float: left;
}
.product_variants h2 {
	color: olive;
	margin-bottom: 5px;
}
#product_variant {
	background-color: #EEEEEE;
	color: black;
	padding: 5px;
	width: 140px;
	display: block;
	position: relative;
	margin-top: 10px;
	z-index: 2;
	font-size: 12px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	-moz-box-shadow: 0px 0px 5px #000000;
	-webkit-box-shadow: 0px 0px 5px #000000;
	box-shadow: 0px 0px 5px #000000;
}

#product_variant strong {
	font-weight: bold;
}
strong#in_stock {
	color: #008A00;
}
strong#out_of_stock {
	color: #E02C00;
}
#product_options {
	background-color: #aaaaaa;
	line-height: 25px;
	padding-left: 5px;
	color: white;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	font-weight: bold;
	font-size: 110%;
}
#product_variant #product_add {
	position: absolute;
	width: 32px;
	height: 32px;
	top: 5px;
	left: 75%;
}
#product_price, #product_stock {
	height: 15px;
}
.product_order {
	display: inline-block;
	vertical-align: top;
	padding-top: 10px;
}

.product_quantity {
	display: inline;
	padding-top: 5px;
}

/* Cart Page */

#cart_empty {
	display: block;
	position: relative;
	width: 400px;
	height: 300px;
	left: 30%;
	text-align: center;
}
#cart_empty p {
	font-size: 16px;
	color: #285199;
}
#cart_overview {
	font-size: 13px;
	width: 90%;
}
#cart_headers {
	color: black;
	font-weight: bold;
	height: 20px;
	text-align: center;
}

#cart_table_items td {
	padding: 10px 5px 8px 5px;
	vertical-align: top;
	border-right: 1px dotted #999;
}
#cart_item_description {
	width: 50%;
	text-align: left;
	align: left;
}
#cart_item_description h4 a {
	font-size: 15px;
	font-weight: bold;
	color: #EB6E44;
}
#cart_item_image {
	text-align: center;
}
#cart_item_image img {
	border: 1px solid black;
}
#cart_item_description p {
	margin-top: 5px;
	display: block;
	font-size: 13px;
	color: #7C786A;
}
#cart_item_delete {
	text-align: center;
}
#cart_item_price {
	text-align: center;
}
#cart_item_quantity {
	text-align: center;
}
#cart_item_total {
	text-align: center;
	border: 0px;
}
#cart_table_items {
}
#cart_table_items tr > td {
	border: 1px solid #BFFF80;
	background-color: #E0FFC2;
}
#cart_table_items tr.alt > td {
	background-color: #C2FFC2;
}
#cart_form {
	padding-left: 50px;
}
#subtotal {
	width: 700px;
	padding-left: 50px;
	padding-right: 50px;
	margin-top: 5px;
}
#subtotal h3 { 
	line-height: 30px;
	font-variant: small-caps;
	font-weight: 100;
	font-size: 16px;
	letter-spacing: 2px;
	color: #000;
	text-align:	right;
	display: inline;
	float: right;
}
#subtotal .links {
	display: inline;
	margin-top: 5px;
	margin-bottom: 8px;
}
#pre_cart_form {
	width: 700px;
	height: 20px;
	margin-bottom: 15px;
	padding-left: 50px;
	text-align: right;
}
#post_cart_form {
	width: 700px;
	margin-top: 20px;
	padding-left: 50px;
	text-align: right;
}
